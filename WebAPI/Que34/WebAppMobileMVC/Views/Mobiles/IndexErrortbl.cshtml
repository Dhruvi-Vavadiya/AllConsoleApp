@model List<WebAppMobileMVC.Models.TblError>

@{
    ViewData["Title"] = "IndexErrortbl";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>IndexErrortbl</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                Id
            </th>
            <th>
               Name
            </th>
            
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            
        </tr>
}
    </tbody>
</table>
